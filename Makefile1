#/codes/Makefile1
#flags: -fdefault-real-8 -c
#file: gaussian_elim_FixMe.F90

OBJECTS = gaussian_elim_FixMe.F90
.PHONY: clean

main.exe: $(OBJECTS)
	gfortran $(OBJECTS) -o main.exe

%.o : %.f90
	gfortran -Wall -cpp -E -dM -Wextra -Wimplicit-interface -fPIC -fmax-errors=1 -g3 -fcheck=all -fbacktrace  -fdefault-real-8 -fdefault-double-8 -c $<

clean:
	rm -f $(OBJECTS) main.exe
